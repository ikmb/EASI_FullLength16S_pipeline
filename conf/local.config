
// Job paramaters to set for this environment

params {
	max_cpus = 4
	max_ram = 15
}

// Resource manager settings

executor {
  queueSize=50
}

// Process setting

process {
  
  executor = 'local'

}

singularity {
  enabled = true
  //runOptions = "-B /work_ifs -B /scratch"
  cacheDir = "${baseDir}/../singularity-cache"
}

